#include "holberton.h"
/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */

char *string_toupper(char *s)
{
	int i = 0;

	for (i = 0; s[i]; i++)
	{
	}
	if (s[i] >= 'A' && s[i] <= 'Z')
		s[i] + 32;
	else if (s[i] >= 'a' && s[i] <= 'Z')
		{
	s[i] - 32;
		}
	return (s);
}
